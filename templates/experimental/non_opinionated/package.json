{
  "name": "espressots-opinionated-experimetal",
  "version": "1.0.0",
  "description": "A template project that uses espressots, nodejs, swc, typescript, eslint and prettier to create fast and reliable web applications.",
  "keywords": [
    "swc",
    "nodejs",
    "expressts",
    "typescript",
    "eslint",
    "prettier",
    "template"
  ],
  "author": "Jo√£o Neto <joaopintoneto@gmail.com>",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node dist/main.js",
    "clean": "rimraf dist",
    "dev": "nodemon",
    "prebuild": "npm run clean",
    "build": "swc src -d dist",
    "prod": "node dist/main.js",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watchAll",
    "format": "eslint \"src/**/*.ts\" --fix",
    "lint": "eslint \"src/**/*.ts\""
  },
  "dependencies": {
    "@expressots/core": "latest"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.76",
    "@swc/jest": "^0.2.29",
    "@swc/register": "^0.1.10",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.10",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "chokidar": "^3.5.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "inversify": "^6.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "inversify-express-utils": "^6.4.3",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6"
  },
  "nodemonConfig": {
    "ext": "*.ts",
    "exec": "node -r @swc/register src/main.ts"
  }
}
